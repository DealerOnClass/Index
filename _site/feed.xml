                &lt;span&gt;&amp;lt;&lt;/span&gt;&lt;span class=&quot;html--element&quot;&gt;Element&amp;nbsp;&lt;/span&gt;&lt;span class=&quot;html--attribute&quot;&gt;Attribute&lt;/span&gt;&lt;span&gt;=&lt;/span&gt;&lt;span&gt;&amp;quot;&lt;/span&gt;&lt;span class=&quot;html--value&quot;&gt;Value&lt;/span&gt;&lt;span&gt;&amp;quot;&lt;/span&gt;&lt;span&gt;&amp;gt;&lt;/span&gt;&lt;span&gt;Content&lt;/span&gt;&lt;span&gt;&amp;lt;/&lt;/span&gt;&lt;span class=&quot;html--element&quot;&gt;Element&lt;/span&gt;&lt;span&gt;&amp;gt;&lt;/span&gt;
