        &lt;/p&gt;
        &lt;p class=&quot;syntax&quot;&gt;
            &lt;code&gt;
                &lt;span&gt;&amp;lt;&lt;/span&gt;&lt;span class=&quot;html--element&quot;&gt;h1&amp;nbsp;&lt;/span&gt;&lt;span class=&quot;html--attribute&quot;&gt;class&lt;/span&gt;&lt;span&gt;=&lt;/span&gt;&lt;span&gt;&amp;quot;&lt;/span&gt;&lt;span class=&quot;html--value&quot;&gt;panel-heading&lt;/span&gt;&lt;span&gt;&amp;quot;&lt;/span&gt;&lt;span&gt;&amp;gt;&lt;/span&gt;&lt;span&gt;Chicken&lt;/span&gt;&lt;span&gt;&amp;lt;/&lt;/span&gt;&lt;span class=&quot;html--element&quot;&gt;h1&lt;/span&gt;&lt;span&gt;&amp;gt;&lt;/span&gt;
