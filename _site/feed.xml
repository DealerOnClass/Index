            &lt;/tr&gt;
            &lt;tr&gt;
                &lt;td&gt;
                    &lt;p class=&quot;syntax&quot;&gt;
                        &lt;code&gt;
                            &lt;span&gt;&amp;lt;&lt;/span&gt;&lt;span class=&quot;html--element&quot;&gt;h4&lt;/span&gt;&lt;span&gt;&amp;gt;&lt;/span&gt;
